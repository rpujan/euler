    "from itertools import permutations\n",
    "perms = [''.join(p) for p in permutations('0123456789')]\n",
    "permsGreater=[]\n",
    "for i in range(len(perms)):\n",
    "\tpermsGreater.append(perms[i])\n",
    " \n",
    "print(permsGreater[999999]) # one millionth number"
   ]
